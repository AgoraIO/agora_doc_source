<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="${ask('Set API ID', String, 'ID', @apiID)}">
    <title><ph keyref="${answer(@apiID)}"/></title>
    <shortdesc id="short"><ph id="shortdesc"><?oxy-placeholder content="Provide a short description for this API."?></ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="${answer(@apiID)}" />
            </keywords>
        </metadata>
    </prolog>
    <refbody><section id="prototype">
        <codeblock props="android"></codeblock>
        <codeblock props="ios mac"></codeblock>
        <codeblock props="windows"></codeblock>
        <codeblock props="electron"></codeblock>
        <codeblock props="unity"></codeblock>
        <codeblock props="rn"></codeblock>
        <codeblock props="flutter"></codeblock>
        </section>
        <section id="detailed_desc">
            <dl outputclass="since">
                <dlentry>
                    <dt>自从</dt>
                    <dd>v</dd>
                </dlentry>
            </dl>
            <p></p>
        </section>
        <section id="parameters"><title>参数</title>
            <parml>
                <plentry>
                    <pt><?oxy-placeholder content="The name of the parameter."?></pt>
                    <pd><?oxy-placeholder content="The description of the parameter. Please specify if the parameter is optional, output, and has a default value or value range."?></pd>
                </plentry>
            </parml>
        </section>
        <section id="return_values">
            <title>返回值</title>
        </section></refbody>
</reference>
