        <section id="detailed_desc" deliveryTarget="details" otherprops="no-title"/>
        <section id="parameters" deliveryTarget="details" props="electron flutter rn unity">
            <title>Parameters</title>
            <parml>
                <plentry props="electron">
                    <pt>cacheSize</pt>
                    <pd>The maximum total cache size for media files, in bytes. The default is 1 GB.</pd>
                </plentry>
                <plentry props="rn">
                    <pt>cacheSize</pt>
                    <pd>The upper limit of the total cache size for cached media files, in bytes. The default is 1 GB.</pd>
                </plentry>
                <plentry props="flutter">
                    <pt>cacheSize</pt>
                    <pd>The total cache size limit for media files, in bytes. The default is 1 GB.</pd>
                </plentry>
                <plentry props="unity">
                    <pt>cacheSize</pt>
                    <pd>The maximum total cache size of cached media files in bytes. The default is 1 GB.</pd>
                </plentry>
            </parml>
        </section>
        <section id="return_values" props="electron flutter rn unity">
            <title>Return Values</title>
            <p props="electron">
                <ul>
                    <li>0: Success.</li>
                    <li>&lt; 0: Failure. See <xref keyref="MEDIA_PLAYER_REASON"/>.</li>
                </ul>
            </p>
