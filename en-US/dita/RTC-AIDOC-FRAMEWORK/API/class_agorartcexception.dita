<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="class_agorartcexception">
    <title><ph keyref="AgoraRtcException"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="flutter">Error code and description.</ph></shortdesc>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="flutter" outputclass="language-dart">class AgoraRtcException implements Exception {
  AgoraRtcException({required this.code, this.message});
  final int code;
  final String? message;
  @override
  String toString() =&gt; 'AgoraRtcException($code, $message)';
}</codeblock>
            </p>
        </section>
        <section id="parameters" deliveryTarget="details" props="flutter">
            <title>Properties</title>
            <parml>
                <plentry props="flutter">
                    <pt>code</pt>
                    <pd>Error code. See <xref keyref="ERROR_CODE_TYPE"/>.</pd>
                </plentry>
                <plentry props="flutter">
                    <pt>message</pt>
                    <pd>Error description.</pd>
                </plentry>
            </parml>
        </section>
    </refbody>
</reference>