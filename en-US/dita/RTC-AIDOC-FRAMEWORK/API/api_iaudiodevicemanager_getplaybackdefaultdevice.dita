<reference id="api_iaudiodevicemanager_getplaybackdefaultdevice">
    <title><ph keyref="getPlaybackDefaultDevice"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="electron">Gets the system default playback device.</ph><ph id="shortdesc" props="flutter">Gets the system default audio playback device.</ph><ph id="shortdesc" props="unity">Gets the system default audio playback device.</ph></shortdesc>
    <prolog>
        <metadata>
