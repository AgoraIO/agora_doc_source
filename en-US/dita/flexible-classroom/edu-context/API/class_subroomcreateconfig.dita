<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="class_subroomcreateconfig">
    <title><ph keyref="AgoraEduContextSubRoomCreateConfig"/></title>
    <shortdesc id="short"><ph id="shortdesc">The configuration of creating a
        sub-room.</ph></shortdesc>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="android" outputclass="language-kotlin"/>
                <codeblock props="ios mac" outputclass="language-swift"/>
                <codeblock props="windows" outputclass="language-cpp"/>
                <codeblock props="web" outputclass="language-typescript"/>
            </p>
        </section>
        <section id="parameters">
            <title><text conref="../conref/conref_api_metadata.dita#metadata/property"/></title>
            <parml>
                <plentry>
                    <pt>subRoomName</pt>
                    <pd>The sub-room name.</pd>
                </plentry>
                <plentry>
                    <pt>invitationUserList</pt>
                    <pd>The list of user IDs who are invited to join the sub-room. This parameter
                        can be null.</pd>
                </plentry>
                <plentry>
                    <pt>subRoomProperties</pt>
                    <pd>The custom properties of the sub-room. This parameter can be null.</pd>
                </plentry>
            </parml>
        </section>
    </refbody>
</reference>
