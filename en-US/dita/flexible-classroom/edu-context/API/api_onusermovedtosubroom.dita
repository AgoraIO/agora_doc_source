            <parml>
                <plentry>
                    <pt>userList</pt>
                    <pd>A list of user IDs.</pd>
                </plentry>
                <plentry>
                    <pt>fromSubRoomUuid</pt>
                    <pd>The source sub-room ID.</pd>
                </plentry>
                <plentry>
                    <pt>toSubRoomUuid</pt>
                    <pd>The target sub-room ID.</pd>
                </plentry>
                <plentry>
                    <pt>operator</pt>
                    <pd>The operator, which can be empty.</pd>
                </plentry>
            </parml>
