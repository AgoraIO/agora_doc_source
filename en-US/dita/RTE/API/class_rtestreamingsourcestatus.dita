<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="class_rtestreamingsourcestatus">
    <title>RteStreamingSourceStatus</title>
    <shortdesc id="short"><ph id="shortdesc">The RTE streaming source status.</ph></shortdesc>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
            <codeblock props="android" outputclass="language-java"></codeblock>
            <codeblock props="ios mac" outputclass="language-objectivec"></codeblock>
            <codeblock props="windows" outputclass="language-cpp">struct RteStreamingSourceStatus {
  int32_t curr_file_id;        
  std::string curr_file_url;                                  
                               
                               
  int64_t curr_file_duration;  
  int32_t curr_file_index;     
  int64_t curr_file_begin_time;  
                                 

  agora::rtc::STREAMING_SRC_STATE
      source_state;      
  int64_t progress_pos;  
};</codeblock>
            <codeblock props="electron" outputclass="language-typescript"></codeblock>
            <codeblock props="unity" outputclass="language-csharp"></codeblock>
            <codeblock props="rn" outputclass="language-typescript"></codeblock>
            <codeblock props="flutter" outputclass="language-dart"></codeblock>
            </p>
        </section>
        <section id="detailed_desc">
            <dl outputclass="since">
                <dlentry>
                    <dt>Since</dt>
                    <dd>v</dd>
                </dlentry>
            </dl>
        </section>
        <section id="parameters">
            <title><text conref="../conref/conref_api_metadata.dita#metadata/property"/></title>
            <parml>
                <plentry>
                    <pt>curr_file_id</pt>
                    <pd>The ID of the current media file. </pd>
                </plentry>
                <plentry>
                    <pt>curr_file_url</pt>
                    <pd>The URL of the current media file. <ul>
                        <li>For a local file, this memeber is the absolute path to this file.</li>
                        <li>For an online file, this memeber is an HTTP or HTTPS URL.</li>
                    </ul></pd>
                </plentry>
                <plentry>
                    <pt>current_file_duration</pt>
                    <pd>The duration (ms) of the current media file.</pd>
                </plentry>
                <plentry>
                    <pt>curr_file_index</pt>
                    <pd>The file index in the playlist.</pd>
                </plentry>
                <plentry>
                    <pt>curr_file_begin_time</pt>
                    <pd>The beginning timestamp (ms) of the current file in the playlist timeline.</pd>
                </plentry>
                <plentry>
                    <pt>source_state</pt>
                    <pd>The state of the current file: <xref keyref="STREAMING_SRC_STATE"/>.</pd>
                </plentry>
                <plentry>
                    <pt>progress_pos</pt>
                    <pd>The playback position (ms) of the current file.</pd>
                </plentry>
            </parml>
        </section>
    </refbody>
</reference>
