    <title>RtcStreamOptions</title>
    <shortdesc id="short"><ph id="shortdesc">Options to configure the RTC stream.</ph></shortdesc>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
            <codeblock props="android" outputclass="language-java"></codeblock>
            <codeblock props="ios mac" outputclass="language-objectivec"></codeblock>
            <codeblock props="windows" outputclass="language-cpp">struct RtcStreamOptions : public StreamOption {
  RtcStreamOptions() : StreamOption(StreamType::kRtcStream), token("") {}

  RtcStreamOptions(const std::string&amp; input_token)
    : StreamOption(StreamType::kRtcStream), token(input_token) {}


  void Assign(const StreamOption&amp; option) override {
    if (option.type == StreamType::kRtcStream) {
      token = static_cast&lt;const RtcStreamOptions&amp;>(option).token;
    }
  }


  std::string token;
};</codeblock>
