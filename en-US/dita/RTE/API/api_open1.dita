        <section id="detailed_desc">
            <dl outputclass="since">
                <dlentry>
                    <dt>Since</dt>
                    <dd>v</dd>
                </dlentry>
            </dl>
        </section>
        <section id="parameters"><title>Parameters</title>
            <parml>
                <plentry>
                    <pt>url</pt>
                    <pd>The URL of the file to be opened. </pd>
                    <pd>For a local file, it's an asbolute path.</pd>
                    <pd>For a cloud file, it's a http or https URL (we support HTTP and HTTPS
                        protocol).</pd>
                </plentry>
                <plentry>
                    <pt>start_pos</pt>
                    <pd>The start position (ms) of the file to be opened, which ranges from 0 to
                        file_duration.</pd>
                </plentry>
            </parml>
        </section>
        <section id="return_values">
            <title>Returns</title>
            <p>
                <ul id="ul_fjm_1lf_3qb">
                    <li>0: Success.</li>
                    <li>&lt; 0: Failure.</li>
                </ul>
            </p>
        </section>
