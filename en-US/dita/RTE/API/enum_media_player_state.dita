    <shortdesc id="short"><ph id="shortdesc">The playback state.</ph></shortdesc>
    <refbody>
        <section id="detailed_desc">
            <dl outputclass="since">
                <dlentry>
                    <dt>Since</dt>
                    <dd>v</dd>
                </dlentry>
            </dl>
        </section>
        <section id="parameters">
            <title>Enumerator</title>
            <parml>
                <plentry>
                    <pt><ph keyref="PLAYER_STATE_IDLE"/></pt>
                    <pd>0: Default state.</pd>
                </plentry>
                <plentry>
                    <pt><ph keyref="PLAYER_STATE_OPENING"/></pt>
                    <pd>Opening the media file.</pd>
                </plentry>
                <plentry>
                    <pt><ph keyref="PLAYER_STATE_OPEN_COMPLETED"/></pt>
                    <pd>The media file is opened successfully.</pd>
