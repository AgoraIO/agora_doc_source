<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="api_enablevideofilter">
    <title><ph keyref="EnableVideoFilter"/></title>
    <shortdesc id="short"><ph id="shortdesc">Enables or disables the video filter for
            pre-processing.</ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="EnableVideoFilter"/>
            </keywords>
        </metadata>
    </prolog>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="android" outputclass="language-java"/>
                <codeblock props="ios mac" outputclass="language-objectivec"/>
                <codeblock props="windows" outputclass="language-cpp">virtual int EnableVideoFilter(const std::string&amp; id, bool enable) = 0;</codeblock>
                <codeblock props="electron" outputclass="language-typescript"/>
                <codeblock props="unity" outputclass="language-csharp"/>
                <codeblock props="rn" outputclass="language-typescript"/>
                <codeblock props="flutter" outputclass="language-dart"/>
            </p>
        </section>
        <section id="detailed_desc">
            <dl outputclass="since">
                <dlentry>
                    <dt>Since</dt>
                    <dd>v</dd>
                </dlentry>
            </dl>
        </section>
        <section id="parameters"><title>Parameters</title>
            <parml>
                <plentry>
                    <pt>id</pt>
                    <pd>The ID of the video filter.</pd>
                </plentry>
                <plentry>
                    <pt>enable</pt>
                    <pd>Whether to enable the video filter:<ul>
                            <li><codeph><ph keyref="true"/></codeph>: Enable the video filter.</li>
                            <li><codeph><ph keyref="false"/></codeph>: Disable the video filter.</li>
                        </ul></pd>
                </plentry>
            </parml>
        </section>
        <section id="return_values">
            <title>Returns</title>
            <ul>
                <li>0: Success.</li>
                <li>&lt; 0: Failure.</li>
            </ul>
        </section>
    </refbody>
</reference>
