<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="api_imediaplayercachemanager_setcachedir">
    <title><ph keyref="setCacheDir"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="cpp">Sets the storage path for cached media files.</ph><ph id="shortdesc" props="android">Sets the storage path for cached media files.</ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="setCacheDir"/>
            </keywords>
        </metadata>
    </prolog>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="cpp" outputclass="language-cpp">virtual int setCacheDir(const char *path) = 0;</codeblock>
                <codeblock props="android" outputclass="language-java">int setCacheDir(String path);</codeblock>
            </p>
        </section>
        <section id="detailed_desc" deliveryTarget="details" otherprops="no-title">
            <note props="cpp">Make sure to initialize <xref keyref="IRtcEngine"/> before calling this method.</note>
            <note props="android">Make sure to initialize <xref keyref="IRtcEngine"/> before calling this method.</note>
        </section>
        <section id="parameters" deliveryTarget="details" props="android cpp">
            <title>Parameters</title>
            <parml>
                <plentry props="cpp">
                    <pt>path</pt>
                    <pd>The absolute path to store cached media files.
                        <note>Ensure the directory exists and is writable.</note>
                    </pd>
                </plentry>
                <plentry props="android">
                    <pt>path</pt>
                    <pd>The absolute path for storing cached media files. Ensure the directory exists and is writable.</pd>
                </plentry>
            </parml>
        </section>
        <section id="return_values" props="android cpp">
            <title>Return Values</title>
            <p props="cpp">
                <ul>
                    <li>0: Success.</li>
                    <li>&lt; 0: Failure. See <xref keyref="MEDIA_PLAYER_REASON"/>.</li>
                </ul>
            </p>
            <p props="android">
                <ul>
                    <li>0: Success.</li>
                    <li>&lt; 0: Failure. See <xref keyref="MEDIA_PLAYER_REASON"/>.</li>
                </ul>
            </p>
        </section>
    </refbody>
</reference>