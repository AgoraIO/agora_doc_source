<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="enum_audiosourcetype">
    <title><ph keyref="AUDIO_SOURCE_TYPE"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="cpp">Audio source type.</ph><ph id="shortdesc" props="android">Audio source type.</ph></shortdesc>
    <refbody>
        <section id="parameters" deliveryTarget="details" props="android cpp">
            <title>Enumerations</title>
            <parml>
                <plentry props="cpp">
                    <pt>AUDIO_SOURCE_MICROPHONE</pt>
                    <pd>(0): Default audio source, captures microphone audio.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>AUDIO_SOURCE_CUSTOM</pt>
                    <pd>(1): Custom audio source.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>AUDIO_SOURCE_MEDIA_PLAYER</pt>
                    <pd>(2): Media player audio source.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>AUDIO_SOURCE_LOOPBACK_RECORDING</pt>
                    <pd>(3): System audio source captured during screen sharing.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>AUDIO_SOURCE_MIXED_STREAM</pt>
                    <pd>(4): Audio captured from mixed source.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>AUDIO_SOURCE_REMOTE_USER</pt>
                    <pd>(5): Audio source from a specified remote user.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>AUDIO_SOURCE_REMOTE_CHANNEL</pt>
                    <pd>(6): Mixed audio source from all users in the current channel.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>AUDIO_SOURCE_UNKNOWN</pt>
                    <pd>(100): Unknown audio source.</pd>
                </plentry>
                <plentry props="android">
                    <pt>AUDIO_SOURCE_MICROPHONE</pt>
                    <pd>(0): Default audio source is the microphone.</pd>
                </plentry>
                <plentry props="android">
                    <pt>AUDIO_SOURCE_CUSTOM</pt>
                    <pd>(1): Custom audio source.</pd>
                </plentry>
                <plentry props="android">
                    <pt>AUDIO_SOURCE_MEDIA_PLAYER</pt>
                    <pd>(2): Media player audio source.</pd>
                </plentry>
                <plentry props="android">
                    <pt>AUDIO_SOURCE_LOOPBACK_RECORDING</pt>
                    <pd>(3): System audio source captured during screen sharing.</pd>
                </plentry>
                <plentry props="android">
                    <pt>AUDIO_SOURCE_MIXED_STREAM</pt>
                    <pd>(4): Mixed audio source from the local client.</pd>
                </plentry>
                <plentry props="android">
                    <pt>AUDIO_SOURCE_REMOTE_USER</pt>
                    <pd>(5): Audio source of a specified remote user.</pd>
                </plentry>
                <plentry props="android">
                    <pt>AUDIO_SOURCE_REMOTE_CHANNEL</pt>
                    <pd>(6): Mixed audio source of all users in the current channel.</pd>
                </plentry>
                <plentry props="android">
                    <pt>AUDIO_SOURCE_UNKNOWN</pt>
                    <pd>(100): Unknown audio source.</pd>
                </plentry>
            </parml>
        </section>
    </refbody>
</reference>