<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="class_advancedaudiooptions">
    <title><ph keyref="AdvancedAudioOptions"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="cpp">Used to configure advanced audio options.</ph><ph id="shortdesc" props="android">Used to configure advanced audio settings.</ph></shortdesc>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="cpp" outputclass="language-cpp">struct AdvancedAudioOptions {
  Optional&lt;int&gt; audioProcessingChannels;
};</codeblock>
                <codeblock props="android" outputclass="language-java">public class AdvancedAudioOptions {
  public AudioProcessingChannelsEnum audioProcessingChannels;
}</codeblock>
            </p>
        </section>
        <section id="parameters" deliveryTarget="details" props="android cpp">
            <title>Properties</title>
            <parml>
                <plentry props="cpp">
                    <pt>audioProcessingChannels</pt>
                    <pd>Number of channels for audio preprocessing:
                        <ul>
                            <li>1: Mono.</li>
                            <li>2: Stereo.</li>
                        </ul>
                    </pd>
                </plentry>
                <plentry props="android">
                    <pt>audioProcessingChannels</pt>
                    <pd>Number of channels for audio preprocessing. See <xref keyref="AUDIO_PROCESSING_CHANNELS"/>.</pd>
                </plentry>
            </parml>
        </section>
    </refbody>
</reference>