<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="api_ivideoeffectobject_removevideoeffect">
    <title><ph keyref="removeVideoEffect"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="cpp">Removes the video effect with the specified node ID.</ph><ph id="shortdesc" props="android">Removes the video effect node for the specified node ID.</ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="removeVideoEffect"/>
            </keywords>
        </metadata>
    </prolog>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="cpp" outputclass="language-cpp">virtual int removeVideoEffect(uint32_t nodeId) = 0;</codeblock>
                <codeblock props="android" outputclass="language-java">int removeVideoEffect(int nodeId);</codeblock>
            </p>
        </section>
        <section id="detailed_desc" deliveryTarget="details" otherprops="no-title">
            <dl outputclass="since">
                <dlentry props="cpp">
                    <dt>Since</dt>
                    <dd>Available since v4.6.0.</dd>
                </dlentry>
                <dlentry props="android">
                    <dt>Since</dt>
                    <dd>Available since v4.6.0.</dd>
                </dlentry>
            </dl>
        </section>
        <section id="parameters" deliveryTarget="details" props="android cpp">
            <title>Parameters</title>
            <parml>
                <plentry props="cpp">
                    <pt>nodeId</pt>
                    <pd>Unique identifier of the video effect node to be removed. See <xref keyref="VIDEO_EFFECT_NODE_ID"/>.</pd>
                </plentry>
                <plentry props="android">
                    <pt>nodeId</pt>
                    <pd>Unique identifier of the video effect node to be removed.</pd>
                </plentry>
            </parml>
        </section>
        <section id="return_values" props="android cpp">
            <title>Return Values</title>
            <p props="cpp">
                <ul>
                    <li>0: Success.</li>
                    <li>&lt; 0: Failure.</li>
                </ul>
            </p>
            <p props="android">
                <ul>
                    <li>0: Success.</li>
                    <li>&lt; 0: Failure.</li>
                </ul>
            </p>
        </section>
    </refbody>
</reference>