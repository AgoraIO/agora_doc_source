<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="class_preloadstate_android">
    <title><ph keyref="PreloadState_android"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="android"><xref keyref="PreloadState_android"/> represents the loading status of a music resource.</ph></shortdesc>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="android" outputclass="language-java">class PreloadState {
    public static final int PRELOAD_STATE_COMPLETED = 0;
    public static final int PRELOAD_STATE_FAILED = 1;
    public static final int PRELOAD_STATE_PRELOADING = 2;
    public static final int PRELOAD_STATE_REMOVED = 3;
  }</codeblock>
            </p>
        </section>
        <section id="parameters" deliveryTarget="details" props="android">
            <title>Properties</title>
            <parml>
                <plentry props="android">
                    <pt>PRELOAD_STATE_COMPLETED</pt>
                    <pd>0: Music resource loading completed.</pd>
                </plentry>
                <plentry props="android">
                    <pt>PRELOAD_STATE_FAILED</pt>
                    <pd>1: Music resource loading failed.</pd>
                </plentry>
                <plentry props="android">
                    <pt>PRELOAD_STATE_PRELOADING</pt>
                    <pd>2: Music resource is loading.</pd>
                </plentry>
                <plentry props="android">
                    <pt>PRELOAD_STATE_REMOVED</pt>
                    <pd>3: Cached music resource has been removed.</pd>
                </plentry>
            </parml>
        </section>
    </refbody>
</reference>