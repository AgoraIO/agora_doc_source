<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="callback_irtcengineeventhandler_onlastmileproberesult">
    <title><ph keyref="onLastmileProbeResult"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="cpp">Callback for uplink and downlink last-mile network probe results.</ph><ph id="shortdesc" props="android">Callback for uplink and downlink Last-mile network probe test results.</ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="onLastmileProbeResult"/>
            </keywords>
        </metadata>
    </prolog>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="cpp" outputclass="language-cpp">virtual void onLastmileProbeResult(const LastmileProbeResult&amp; result)</codeblock>
                <codeblock props="android" outputclass="language-java">public void onLastmileProbeResult(LastmileProbeResult result) {}</codeblock>
            </p>
        </section>
        <section id="detailed_desc" deliveryTarget="details" otherprops="no-title">
            <p props="cpp">After calling <xref keyref="startLastmileProbeTest"/>, the SDK triggers this callback within 30 seconds.</p>
            <p props="android">This callback is triggered after calling <xref keyref="startLastmileProbeTest"/>.</p>
        </section>
        <section id="timing" deliveryTarget="details" props="android">
            <title>Trigger Timing</title>
            <p props="android">This callback is triggered within 30 seconds after calling <xref keyref="startLastmileProbeTest"/>.</p>
        </section>
        <section id="parameters" deliveryTarget="details" props="android cpp">
            <title>Parameters</title>
            <parml>
                <plentry props="cpp">
                    <pt>result</pt>
                    <pd>Uplink and downlink last-mile network probe test result. See <xref keyref="LastmileProbeResult"/>.</pd>
                </plentry>
                <plentry props="android">
                    <pt>result</pt>
                    <pd>Uplink and downlink Last-mile network probe test results. See <xref keyref="LastmileProbeResult"/>.</pd>
                </plentry>
            </parml>
        </section>
    </refbody>
</reference>