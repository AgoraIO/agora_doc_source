<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="callback_irtcengineeventhandler_onaudioeffectfinished">
    <title><ph keyref="onAudioEffectFinished"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="cpp">Callback triggered when the local audio effect file finishes playing.</ph><ph id="shortdesc" props="android">Callback when local audio effect file finishes playing.</ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="onAudioEffectFinished"/>
            </keywords>
        </metadata>
    </prolog>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="cpp" outputclass="language-cpp">virtual void onAudioEffectFinished(int soundId) {}</codeblock>
                <codeblock props="android" outputclass="language-java">public void onAudioEffectFinished(int soundId) {}</codeblock>
            </p>
        </section>
        <section id="detailed_desc" deliveryTarget="details" otherprops="no-title">
            <p props="cpp">This callback is triggered when the local audio effect file finishes playing. You can use this callback to determine whether the specified audio effect has completed playback.</p>
        </section>
        <section id="timing" deliveryTarget="details" props="android">
            <title>Trigger Timing</title>
            <p props="android">This callback is triggered when the local audio effect file finishes playing.</p>
        </section>
        <section id="parameters" deliveryTarget="details" props="android cpp">
            <title>Parameters</title>
            <parml>
                <plentry props="cpp">
                    <pt>soundId</pt>
                    <pd>Audio effect ID, a unique identifier for each audio effect file.</pd>
                </plentry>
                <plentry props="android">
                    <pt>soundId</pt>
                    <pd>The unique ID of the audio effect file.</pd>
                </plentry>
            </parml>
        </section>
    </refbody>
</reference>