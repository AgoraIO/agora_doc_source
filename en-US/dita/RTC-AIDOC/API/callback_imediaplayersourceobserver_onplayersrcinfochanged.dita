<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="callback_imediaplayersourceobserver_onplayersrcinfochanged">
    <title><ph keyref="onPlayerSrcInfoChanged"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="cpp">Callback that occurs when the video bitrate of the media source changes.</ph><ph id="shortdesc" props="android">Callback for video bitrate changes of the media resource.</ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="onPlayerSrcInfoChanged"/>
            </keywords>
        </metadata>
    </prolog>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="cpp" outputclass="language-cpp">virtual void onPlayerSrcInfoChanged(const media::base::SrcInfo&amp; from, const media::base::SrcInfo&amp; to) = 0;</codeblock>
                <codeblock props="android" outputclass="language-java">void onPlayerSrcInfoChanged(SrcInfo from, SrcInfo to)</codeblock>
            </p>
        </section>
        <section id="detailed_desc" deliveryTarget="details" otherprops="no-title"/>
        <section id="timing" deliveryTarget="details" props="android">
            <title>Trigger Timing</title>
            <p props="android">This callback is triggered when the video bitrate of the media resource changes.</p>
        </section>
        <section id="parameters" deliveryTarget="details" props="android cpp">
            <title>Parameters</title>
            <parml>
                <plentry props="cpp">
                    <pt>from</pt>
                    <pd>Video bitrate information of the currently playing media source. See <xref keyref="SrcInfo"/>.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>to</pt>
                    <pd>Video bitrate information after the change. See <xref keyref="SrcInfo"/>.</pd>
                </plentry>
                <plentry props="android">
                    <pt>from</pt>
                    <pd>Video bitrate information of the media resource being played. See <xref keyref="SrcInfo"/>.</pd>
                </plentry>
                <plentry props="android">
                    <pt>to</pt>
                    <pd>Video bitrate information after the change. See <xref keyref="SrcInfo"/>.</pd>
                </plentry>
            </parml>
        </section>
    </refbody>
</reference>