<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="api_imediaplayer_unloadsrc">
    <title><ph keyref="unloadSrc"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="cpp">Unloads preloaded media resources.</ph><ph id="shortdesc" props="android">Unloads a preloaded media resource.</ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="unloadSrc"/>
            </keywords>
        </metadata>
    </prolog>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="cpp" outputclass="language-cpp">virtual int unloadSrc(const char* src) = 0;</codeblock>
                <codeblock props="android" outputclass="language-java">int unloadSrc(String src);</codeblock>
            </p>
        </section>
        <section id="detailed_desc" deliveryTarget="details" otherprops="no-title">
            <note props="cpp">The <xref keyref="unloadSrc"/> method cannot release media resources that are currently playing.</note>
            <note props="android"><xref keyref="unloadSrc"/> cannot release a media resource that is currently playing.</note>
        </section>
        <section id="parameters" deliveryTarget="details" props="android cpp">
            <title>Parameters</title>
            <parml>
                <plentry props="cpp">
                    <pt>src</pt>
                    <pd>The URL of the media resource.</pd>
                </plentry>
                <plentry props="android">
                    <pt>src</pt>
                    <pd>URL of the media resource.</pd>
                </plentry>
            </parml>
        </section>
        <section id="return_values" props="android cpp">
            <title>Return Values</title>
            <p props="cpp">
                <ul>
                    <li>0: Success.</li>
                    <li>&lt; 0: Failure.</li>
                </ul>
            </p>
            <p props="android">
                <ul>
                    <li>0: Success.</li>
                    <li>&lt; 0: Failure.</li>
                </ul>
            </p>
        </section>
    </refbody>
</reference>