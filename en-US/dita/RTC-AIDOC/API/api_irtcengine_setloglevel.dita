<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="api_irtcengine_setloglevel">
    <title><ph keyref="setLogLevel"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="cpp">Sets the SDK log level.</ph><ph id="shortdesc" props="android">Sets the SDK log output level.</ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="setLogLevel"/>
            </keywords>
        </metadata>
    </prolog>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="cpp" outputclass="language-cpp">virtual int setLogLevel(commons::LOG_LEVEL level) = 0;</codeblock>
                <codeblock props="android" outputclass="language-java">public abstract int setLogLevel(int level);</codeblock>
            </p>
        </section>
        <section id="detailed_desc" deliveryTarget="details" otherprops="no-title">
            <dl outputclass="deprecated">
                <dlentry props="cpp">
                    <dt>Deprecated</dt>
                    <dd>This method is deprecated. Use the <codeph>context</codeph> parameter in <xref keyref="initialize"/> to set the log level instead.</dd>
                </dlentry>
                <dlentry props="android">
                    <dt>Deprecated</dt>
                    <dd>This method is deprecated. Set the log output level using the <codeph>config</codeph> parameter when calling <xref keyref="initialize"/>.</dd>
                </dlentry>
            </dl>
            <p props="cpp">Select a log level to view logs of that level and above.</p>
            <p props="android">Select a log level to view logs of that level and higher.</p>
        </section>
        <section id="parameters" deliveryTarget="details" props="android cpp">
            <title>Parameters</title>
            <parml>
                <plentry props="cpp">
                    <pt>level</pt>
                    <pd>The log level. See <xref keyref="LOG_LEVEL"/>.</pd>
                </plentry>
                <plentry props="android">
                    <pt>level</pt>
                    <pd>The log level. See <xref keyref="LOG_LEVEL"/>.</pd>
                </plentry>
            </parml>
        </section>
        <section id="return_values" props="android cpp">
            <title>Return Values</title>
            <p props="cpp">
                <ul>
                    <li>0: Success.</li>
                    <li>&lt; 0: Failure.</li>
                </ul>
            </p>
            <p props="android">
                <ul>
                    <li>0: Success.</li>
                    <li>&lt; 0: Failure.</li>
                </ul>
            </p>
        </section>
    </refbody>
</reference>