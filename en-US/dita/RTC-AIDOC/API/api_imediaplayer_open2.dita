<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="api_imediaplayer_open2">
    <title><ph keyref="open2"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="android">Opens a media file via URI.</ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="open2"/>
            </keywords>
        </metadata>
    </prolog>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="android" outputclass="language-java">int open(Uri uri, long startPos);</codeblock>
            </p>
        </section>
        <section id="detailed_desc" deliveryTarget="details" otherprops="no-title">
            <note props="android">This method is asynchronous.</note>
        </section>
        <section id="parameters" deliveryTarget="details" props="android">
            <title>Parameters</title>
            <parml>
                <plentry props="android">
                    <pt>uri</pt>
                    <pd>The URI of the media file.</pd>
                </plentry>
                <plentry props="android">
                    <pt>startPos</pt>
                    <pd>The start position for playback in milliseconds. (Default is 0)</pd>
                </plentry>
            </parml>
        </section>
        <section id="return_values" props="android">
            <title>Return Values</title>
            <p props="android">
                <ul>
                    <li>0: Success.</li>
                    <li>&lt; 0: Failure.</li>
                </ul>
            </p>
        </section>
    </refbody>
</reference>