<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="enum_videocontenthint">
    <title><ph keyref="VIDEO_CONTENT_HINT"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="cpp">Hint type of screen sharing content.</ph></shortdesc>
    <refbody>
        <section id="parameters" deliveryTarget="details" props="cpp">
            <title>Enumerations</title>
            <parml>
                <plentry props="cpp">
                    <pt>CONTENT_HINT_NONE</pt>
                    <pd>(Default): No content hint.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>CONTENT_HINT_MOTION</pt>
                    <pd>: Motion content. Suitable for scenarios focusing on smoothness or sharing video clips, movies, video games, etc.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>CONTENT_HINT_DETAILS</pt>
                    <pd>: Static content. Suitable for scenarios focusing on clarity or sharing images, PPT slides, text, etc.</pd>
                </plentry>
            </parml>
        </section>
    </refbody>
</reference>