<reference id="enum_networktype">
    <title><ph keyref="NETWORK_TYPE"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="cpp">Network types.</ph></shortdesc>
    <refbody>
        <section id="parameters" deliveryTarget="details" props="cpp">
            <title>Enumerations</title>
            <parml>
                <plentry props="cpp">
                    <pt>NETWORK_TYPE_UNKNOWN</pt>
                    <pd>(-1): Unknown network type.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>NETWORK_TYPE_DISCONNECTED</pt>
                    <pd>(0): The SDK is disconnected from the network.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>NETWORK_TYPE_LAN</pt>
                    <pd>(1): LAN (Local Area Network) type network.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>NETWORK_TYPE_WIFI</pt>
                    <pd>(2): Wi-Fi type network (including hotspots).</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>NETWORK_TYPE_MOBILE_2G</pt>
                    <pd>(3): Mobile 2G network.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>NETWORK_TYPE_MOBILE_3G</pt>
                    <pd>(4): Mobile 3G network.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>NETWORK_TYPE_MOBILE_4G</pt>
                    <pd>(5): Mobile 4G network.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>NETWORK_TYPE_MOBILE_5G</pt>
                    <pd>(6): Mobile 5G network.</pd>
                </plentry>
            </parml>
        </section>
    </refbody>
