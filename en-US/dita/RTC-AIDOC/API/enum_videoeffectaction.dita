<reference id="enum_videoeffectaction">
    <title><ph keyref="VIDEO_EFFECT_ACTION"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="cpp">Operation types for video effect nodes.</ph><ph id="shortdesc" props="android">Action types performed on video effect nodes.</ph></shortdesc>
    <refbody>
        <section id="detailed_desc" deliveryTarget="details" otherprops="no-title" props="android cpp">
            <dl outputclass="since">
                <dlentry props="cpp">
                    <dt>Since</dt>
                    <dd>Available since v4.6.0.</dd>
                </dlentry>
                <dlentry props="android">
                    <dt>Since</dt>
                    <dd>Available since v4.6.0.</dd>
                </dlentry>
            </dl>
        </section>
        <section id="parameters" deliveryTarget="details" props="android cpp">
            <title>Enumerations</title>
            <parml>
                <plentry props="cpp">
                    <pt>SAVE</pt>
                    <pd>(1): Save the current parameters of the video effect.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>RESET</pt>
                    <pd>(2): Reset the video effect to default parameters.</pd>
                </plentry>
                <plentry props="android">
                    <pt>SAVE</pt>
                    <pd>(1): Saves the current parameters of the video effect node.</pd>
                </plentry>
                <plentry props="android">
                    <pt>RESET</pt>
                    <pd>(2): Resets the video effect node to default parameters.</pd>
                </plentry>
            </parml>
        </section>
    </refbody>
