            <p outputclass="codeblock">
                <codeblock props="cpp" outputclass="language-cpp">struct LocalSpatialAudioConfig {
  agora::rtc::IRtcEngine* rtcEngine;
};</codeblock>
                <codeblock props="android" outputclass="language-java">public class LocalSpatialAudioConfig {
  public RtcEngine mRtcEngine;
}</codeblock>
            </p>
        </section>
        <section id="parameters" deliveryTarget="details" props="android cpp">
