<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="enum_qualitytype">
    <title><ph keyref="QUALITY_TYPE"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="cpp">Network quality type.</ph></shortdesc>
    <refbody>
        <section id="parameters" deliveryTarget="details" props="cpp">
            <title>Enumerations</title>
            <parml>
                <plentry props="cpp">
                    <pt>QUALITY_UNKNOWN</pt>
                    <pd>(0): Unknown network quality.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>QUALITY_EXCELLENT</pt>
                    <pd>(1): Excellent network quality, best call experience.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>QUALITY_GOOD</pt>
                    <pd>(2): Good network quality, smooth call experience, bitrate may be slightly lower than excellent.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>QUALITY_POOR</pt>
                    <pd>(3): Average network quality, users may notice slight degradation in call quality.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>QUALITY_BAD</pt>
                    <pd>(4): Poor network quality, users cannot have smooth calls.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>QUALITY_VBAD</pt>
                    <pd>(5): Very bad network quality, users can hardly make calls.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>QUALITY_DOWN</pt>
                    <pd>(6): Network connection is lost, users cannot make calls at all.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>QUALITY_UNSUPPORTED</pt>
                    <pd>(7): (Reserved) Unable to detect network quality.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>QUALITY_DETECTING</pt>
                    <pd>(8): Last-mile network probe test is in progress.</pd>
                </plentry>
            </parml>
        </section>
    </refbody>
</reference>