<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="api_irtcengine_resumealleffects">
    <title><ph keyref="resumeAllEffects"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="cpp">Resumes playing all audio effect files.</ph><ph id="shortdesc" props="android">Resumes playback of all audio effect files.</ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="resumeAllEffects"/>
            </keywords>
        </metadata>
    </prolog>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="cpp" outputclass="language-cpp">virtual int resumeAllEffects() = 0;</codeblock>
                <codeblock props="android" outputclass="language-java">public abstract int resumeAllEffects();</codeblock>
            </p>
        </section>
        <section id="detailed_desc" deliveryTarget="details" otherprops="no-title">
            <p props="cpp">After pausing playback by calling <xref keyref="pauseAllEffects"/>, you can call this method to resume playback.</p>
            <p props="android">After calling <xref keyref="pauseAllEffects"/> to pause playback, you can call this method to resume playback.</p>
        </section>
        <section id="timing" deliveryTarget="details" props="android cpp">
            <title>Timing</title>
            <p props="cpp">Call this method after calling <xref keyref="pauseAllEffects"/>.</p>
            <p props="android">Call this method after <xref keyref="pauseAllEffects"/>.</p>
        </section>
        <section id="return_values" props="android cpp">
            <title>Return Values</title>
            <p props="cpp">
                <ul>
                    <li>0: Success.</li>
                    <li>&lt; 0: Failure.</li>
                </ul>
            </p>
            <p props="android">
                <ul>
                    <li>0: Success.</li>
                    <li>&lt; 0: Failure.</li>
                </ul>
            </p>
        </section>
    </refbody>
</reference>