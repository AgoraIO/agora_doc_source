<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="api_imediaplayercachemanager_removeallcaches">
    <title><ph keyref="removeAllCaches"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="cpp">Deletes all cached media files in the media player.</ph><ph id="shortdesc" props="android">Deletes all cached media files in the media player.</ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="removeAllCaches"/>
            </keywords>
        </metadata>
    </prolog>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="cpp" outputclass="language-cpp">virtual int removeAllCaches() = 0;</codeblock>
                <codeblock props="android" outputclass="language-java">int removeAllCaches();</codeblock>
            </p>
        </section>
        <section id="detailed_desc" deliveryTarget="details" otherprops="no-title">
            <note props="cpp"><xref keyref="removeAllCaches"/> does not delete the cached media file that is currently playing.</note>
            <note props="android">The currently playing cached media file will not be deleted.</note>
        </section>
        <section id="return_values" props="android cpp">
            <title>Return Values</title>
            <p props="cpp">
                <ul>
                    <li>0: Success.</li>
                    <li>&lt; 0: Failure. See <xref keyref="MEDIA_PLAYER_REASON"/>.</li>
                </ul>
            </p>
            <p props="android">
                <ul>
                    <li>0: Success.</li>
                    <li>&lt; 0: Failure. See <xref keyref="MEDIA_PLAYER_REASON"/>.</li>
                </ul>
            </p>
        </section>
    </refbody>
</reference>