<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="api_imediaplayercachemanager_removecachebyuri">
    <title><ph keyref="removeCacheByUri"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="cpp">Deletes cached media files.</ph><ph id="shortdesc" props="android">Deletes a cached media file.</ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="removeCacheByUri"/>
            </keywords>
        </metadata>
    </prolog>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="cpp" outputclass="language-cpp">virtual int removeCacheByUri(const char *uri) = 0;</codeblock>
                <codeblock props="android" outputclass="language-java">int removeCacheByUri(String uri);</codeblock>
            </p>
        </section>
        <section id="detailed_desc" deliveryTarget="details" otherprops="no-title">
            <note props="cpp"><xref keyref="removeCacheByUri"/> currently does not delete cached media files that are being played.</note>
            <note props="android">The currently playing cached media file will not be deleted.</note>
        </section>
        <section id="parameters" deliveryTarget="details" props="android cpp">
            <title>Parameters</title>
            <parml>
                <plentry props="cpp">
                    <pt>uri</pt>
                    <pd>The URI (Uniform Resource Identifier) of the media file to delete.</pd>
                </plentry>
                <plentry props="android">
                    <pt>uri</pt>
                    <pd>The URI (Uniform Resource Identifier) of the media file to be deleted.</pd>
                </plentry>
            </parml>
        </section>
        <section id="return_values" props="android cpp">
            <title>Return Values</title>
            <p props="cpp">
                <ul>
                    <li>0: Success.</li>
                    <li>&lt; 0: Failure. See <xref keyref="MEDIA_PLAYER_REASON"/>.</li>
                </ul>
            </p>
            <p props="android">
                <ul>
                    <li>0: Success.</li>
                    <li>&lt; 0: Failure. See <xref keyref="MEDIA_PLAYER_REASON"/>.</li>
                </ul>
            </p>
        </section>
    </refbody>
</reference>