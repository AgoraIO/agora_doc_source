<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="class_encodedaudioframeinfo">
    <title><ph keyref="EncodedAudioFrameInfo"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="cpp">Information about the encoded audio frame.</ph></shortdesc>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="cpp" outputclass="language-cpp">struct EncodedAudioFrameInfo {
  AUDIO_CODEC_TYPE codec;
  int sampleRateHz;
  int samplesPerChannel;
  int numberOfChannels;
  EncodedAudioFrameAdvancedSettings advancedSettings;
  int64_t captureTimeMs;
};</codeblock>
            </p>
        </section>
        <section id="parameters" deliveryTarget="details" props="cpp">
            <title>Properties</title>
            <parml>
                <plentry props="cpp">
                    <pt>codec</pt>
                    <pd>Audio codec type. See <xref keyref="AUDIO_CODEC_TYPE"/>.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>sampleRateHz</pt>
                    <pd>Audio sample rate in Hz.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>samplesPerChannel</pt>
                    <pd>Number of audio samples per channel.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>numberOfChannels</pt>
                    <pd>Number of audio channels.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>advancedSettings</pt>
                    <pd>Advanced settings. Currently not supported. See <codeph>EncodedAudioFrameAdvancedSettings</codeph>.</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>captureTimeMs</pt>
                    <pd>Timestamp of the captured audio frame in milliseconds.</pd>
                </plentry>
            </parml>
        </section>
    </refbody>
</reference>