<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="enum_audiosourcetype">
    <title><ph keyref="AUDIO_SOURCE_TYPE"/></title>
    <shortdesc id="short"><ph id="shortdesc">The audio source type.</ph></shortdesc>
    <refbody>
        <section id="detailed_desc">
            <dl outputclass="since">
                <dlentry props="native">
                    <dt>Since</dt>
                    <dd>v4.5.0</dd>
                </dlentry>
            </dl>
        </section>
        <section id="parameters">
            <title>Enumerator</title>
            <parml>
                <plentry>
                    <pt><ph keyref="AUDIO_SOURCE_MICROPHONE"/></pt>
                    <pd>0: (Default) Microphone.</pd>
                </plentry>
                <plentry>
                    <pt><ph keyref="AUDIO_SOURCE_CUSTOM"/></pt>
                    <pd>1: Custom audio stream.</pd>
                </plentry>
                <plentry>
                    <pt><ph keyref="AUDIO_SOURCE_MEDIA_PLAYER"/></pt>
                    <pd>2: Media player.</pd>
                </plentry>
                <plentry>
                    <pt><ph keyref="AUDIO_SOURCE_LOOPBACK_RECORDING"/></pt>
                    <pd>3: System audio stream captured during screen sharing.</pd>
                </plentry>
                <plentry props="hide">
                    <!--internal use only-->
                    <pt><ph keyref="AUDIO_SOURCE_MIXED_STREAM"/></pt>
                    <pd>4: Audio stream that has been mixed.</pd>
                </plentry>
                <plentry>
                    <pt><ph keyref="AUDIO_SOURCE_REMOTE_USER"/></pt>
                    <pd>5: Audio stream from a specified remote user.</pd>
                </plentry>
                <plentry>
                    <pt><ph keyref="AUDIO_SOURCE_REMOTE_CHANNEL"/></pt>
                    <pd>6: Mixed audio streams from all users in the current channel.</pd>
                </plentry>
                <plentry>
                    <pt><ph keyref="AUDIO_SOURCE_UNKNOWN"/></pt>
                    <pd>100: An unknown audio source.</pd>
                </plentry>
            </parml>
        </section></refbody>
</reference>
