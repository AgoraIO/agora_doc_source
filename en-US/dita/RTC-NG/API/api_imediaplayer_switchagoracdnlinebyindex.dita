<reference id="api_imediaplayer_switchagoracdnlinebyindex">
    <title><ph keyref="switchAgoraCDNLineByIndex" /></title>
    <shortdesc id="short"><ph id="shortdesc">Changes the CDN route for playing the media resource.</ph><ph id="shortdesc"></ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="switchAgoraCDNLineByIndex" />
            </keywords>
