    <title><ph keyref="disableAudioSpectrumMonitor" /></title>
    <shortdesc id="short"><ph id="shortdesc">Disables audio spectrum monitoring.</ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="disableAudioSpectrumMonitor" />
            </keywords>
        </metadata>
    </prolog>
