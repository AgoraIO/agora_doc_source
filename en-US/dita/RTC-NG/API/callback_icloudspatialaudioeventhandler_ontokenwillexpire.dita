<reference id="callback_icloudspatialaudioeventhandler_ontokenwillexpire">
    <title><ph keyref="onTokenWillExpire"/></title>
    <shortdesc id="short"><ph id="shortdesc">Occurs when the RTM token expires.</ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
