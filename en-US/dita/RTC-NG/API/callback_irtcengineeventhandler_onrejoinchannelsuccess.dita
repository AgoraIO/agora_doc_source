                <pt/>
                <pd/>
            </plentry>
            <plentry props="native electron">
                <pt>channel</pt>
                <pd>The channel name.</pd>
            </plentry>
            <plentry id="uid" props="native electron">
                <pt>uid</pt>
                <pd>The ID of the user who rejoins the channel.</pd>
            </plentry>
            <plentry props="unity">
