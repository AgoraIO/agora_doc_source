<reference id="callback_onleavechannel">
    <title><ph keyref="onLeaveChannel"/></title>
    <shortdesc id="short"><ph id="shortdesc">Occurs when a user leaves a channel.</ph></shortdesc>
    <prolog>
        <metadata>
