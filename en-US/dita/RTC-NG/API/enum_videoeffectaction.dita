<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="enum_videoeffectaction">
    <title><ph keyref="VIDEO_EFFECT_ACTION"/></title>
    <shortdesc id="short"><ph id="shortdesc">Operations performed on video effect nodes.</ph></shortdesc>
    <refbody>
        <section id="detailed_desc">
            <dl outputclass="since">
                <dlentry props="native">
                    <dt>Since:</dt>
                    <dd>Available since v4.6.0.</dd>
                </dlentry>
            </dl>
        </section>
        <section id="parameters">
            <title>Enumerator</title>
            <parml>
            <plentry props="native">
                <pt><ph keyref="SAVE"/></pt>
                <pd props="cpp">(1): Saves the current video effect parameters.</pd>
            <pd props="android apple">(1): Saves the current video effect parameter settings.</pd>
            </plentry>
        <plentry props="native">
                <pt><ph keyref="RESET"/></pt>
                <pd props="cpp">(2): Resets the video effect to default parameters.</pd>
            <pd props="apple">(2): Resets the video effect parameters to default values.</pd>
            <pd props="android">(2): Resets the video effect to default parameter settings.</pd>
            </plentry>
        </parml>
        </section></refbody>
</reference>
