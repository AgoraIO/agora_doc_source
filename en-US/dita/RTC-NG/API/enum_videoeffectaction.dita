<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="enum_videoeffectaction">
    <title><ph keyref="VIDEO_EFFECT_ACTION"/></title>
    <shortdesc id="short"><ph id="shortdesc">Actions that can be performed on a video effect node.</ph></shortdesc>
    <refbody>
        <section id="detailed_desc">
            <dl outputclass="since">
                <dlentry props="native">
                    <dt>Since:</dt>
                    <dd>Available since v4.6.0.</dd>
                </dlentry>
            </dl>
        </section>
        <section id="parameters">
            <title>Enumerator</title>
            <parml>
            <plentry props="native">
                <pt><ph keyref="SAVE"/></pt>
                <pd props="apple cpp">(1): Saves the current parameters of the video effect.</pd>
            <pd props="android">(1): Saves the current parameters of the video effect node.</pd>
            </plentry>
        <plentry props="native">
                <pt><ph keyref="RESET"/></pt>
                <pd props="apple cpp">(2): Resets the video effect to its default parameters.</pd>
            <pd props="android">(2): Resets the video effect node to its default parameters.</pd>
            </plentry>
        </parml>
        </section></refbody>
</reference>
