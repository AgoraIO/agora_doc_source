<reference id="enum_mediastreamtype">
    <title><ph keyref="MEDIA_STREAM_TYPE"/></title>
    <shortdesc id="short"><ph id="shortdesc">The type of the media stream.</ph></shortdesc>
    <refbody>
        <section id="parameters">
            <title>Enumerator</title>
            <parml>
                <plentry>
                    <pt><ph keyref="STREAM_TYPE_UNKNOWN"/></pt>
                    <pd>0: The type is unknown.</pd>
                </plentry>
                <plentry>
                    <pt props="android cpp"><ph keyref="STREAM_TYPE_VIDEO"/></pt>
                    <pt props="apple"><ph keyref="AgoraMediaRecorderStreamTypeAudio"/></pt>
                    <pd>1: The video stream.</pd>
                </plentry>
                <plentry>
                    <pt props="android cpp"><ph keyref="STREAM_TYPE_AUDIO"/></pt>
                    <pt props="apple"><ph keyref="AgoraMediaRecorderStreamTypeVideo"/></pt>
                    <pd>2: The audio stream.</pd>
                </plentry>
                <plentry>
                    <pt><ph keyref="STREAM_TYPE_SUBTITLE"/></pt>
                    <pd>3: The subtitle stream.</pd>
                </plentry>
            </parml>
        </section></refbody>
</reference>
