    <title><ph keyref="videoSourceEnableLocalAudio" /></title>
    <shortdesc id="short"><ph id="shortdesc" conkeyref="enableLocalAudio/shortdesc" /></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="videoSourceEnableLocalAudio" />
