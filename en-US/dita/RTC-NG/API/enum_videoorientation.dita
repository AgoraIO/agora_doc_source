<reference id="enum_videoorientation">
    <title><ph keyref="VIDEO_ORIENTATION"></ph></title>
    <shortdesc id="short"><ph id="shortdesc">The clockwise rotation of the video.</ph></shortdesc>
    <refbody>
        <section id="parameters">
            <title>Enumerator</title>
            <parml>
                <plentry>
                    <pt><ph keyref="VIDEO_ORIENTATION_0"/></pt>
                    <pd>0: (Default) No rotation.</pd>
                </plentry>
                <plentry>
                    <pt><ph keyref="VIDEO_ORIENTATION_90"/></pt>
                    <pd>90: 90 degrees.</pd>
                </plentry>
                <plentry>
                    <pt><ph keyref="VIDEO_ORIENTATION_180"/></pt>
                    <pd>180: 180 degrees.</pd>
                </plentry>
                <plentry>
                    <pt><ph keyref="VIDEO_ORIENTATION_270"/></pt>
                    <pd>270: 270 degrees.</pd>
                </plentry>
            </parml>
        </section></refbody>
</reference>
