                <pt props="ios">connection</pt>
                <pd props="android">Connection information. See <xref keyref="RtcConnection"/>.</pd>
                <pd props="ios">Connection information. See <xref keyref="RtcConnection"/>.</pd>
                <pd props="cpp">Connection information. See <xref keyref="RtcConnection"/> for details.</pd>
            </plentry>
