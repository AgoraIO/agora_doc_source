<reference id="api_videosourceenablelocalvideo">
    <title><ph keyref="videoSourceEnableLocalVideo" /></title>
    <shortdesc id="short"><ph id="shortdesc" conkeyref="enableLocalVideo/shortdesc" /></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="videoSourceEnableLocalVideo" />
            </keywords>
