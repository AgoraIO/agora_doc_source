<reference id="api_startscreencapturebywindowid">
    <title><ph keyref="startScreenCaptureByWindowId"/></title>
    <shortdesc id="short"><ph id="shortdesc">Shares the whole or part of a window by specifying the
            window ID.</ph></shortdesc>
    <prolog>
        <metadata>
