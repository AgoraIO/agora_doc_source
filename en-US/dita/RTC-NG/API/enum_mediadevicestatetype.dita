<reference id="enum_mediadevicestatetype">
    <title><ph keyref="MEDIA_DEVICE_STATE_TYPE"/></title>
    <shortdesc id="short"><ph id="shortdesc">Media device states.</ph></shortdesc>
    <refbody>
        <section id="parameters">
            <title>Enumerator</title>
            <parml>
            <plentry>
                <pt><ph keyref="MEDIA_DEVICE_STATE_IDLE"/></pt>
                <pd>0: The device is ready for use.</pd>
            </plentry>
            <plentry>
                <pt><ph keyref="MEDIA_DEVICE_STATE_ACTIVE"/></pt>
                <pd>1: The device is in use.</pd>
            </plentry>
            <plentry>
                <pt><ph keyref="MEDIA_DEVICE_STATE_DISABLED"/></pt>
                <pd>2: The device is disabled.</pd>
            </plentry>
            <plentry props="hide">
                <pt><ph keyref="MEDIA_DEVICE_STATE_PLUGGED_IN"/></pt>
                <pd>3: The device is plugged in.</pd>
            </plentry>
            <plentry>
                <pt><ph keyref="MEDIA_DEVICE_STATE_NOT_PRESENT"/></pt>
                <pd>4: The device is not found.</pd>
            </plentry>
            <plentry>
                <pt><ph keyref="MEDIA_DEVICE_STATE_UNPLUGGED"/></pt>
                <pd>8: The device is unplugged.</pd>
            </plentry>
            </parml> </section>
    </refbody>
</reference>
