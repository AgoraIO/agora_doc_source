<reference id="enum_mediaplayerplaybackspeed">
    <title><ph keyref="MEDIA_PLAYER_PLAYBACK_SPEED"/></title>
    <shortdesc id="short"><ph id="shortdesc">The playback speed.</ph></shortdesc>
    <refbody>
        <section id="parameters">
            <title>Enumerator</title>
            <parml>
            <plentry>
                <pt><ph keyref="PLAYBACK_SPEED_ORIGINAL"/></pt>
                <pd>100: The original playback speed.</pd>
            </plentry>
            <plentry>
                <pt><ph keyref="PLAYBACK_SPEED_50_PERCENT"/></pt>
                <pd>50: The playback speed is 0.50 times the original speed.</pd>
            </plentry>
            <plentry>
                <pt><ph keyref="PLAYBACK_SPEED_75_PERCENT"/></pt>
                <pd>75: The playback speed is 0.75 times the original speed.</pd>
            </plentry>
            <plentry>
                <pt><ph keyref="PLAYBACK_SPEED_125_PERCENT"/></pt>
                <pd>125: The playback speed is 1.25 times the original speed.</pd>
            </plentry>
            <plentry>
                <pt><ph keyref="PLAYBACK_SPEED_150_PERCENT"/></pt>
                <pd>150: The playback speed is 1.50 times the original speed.</pd>
            </plentry>
            <plentry>
                <pt><ph keyref="PLAYBACK_SPEED_200_PERCENT"/></pt>
                <pd>200: The playback speed is 2.00 times the original speed.</pd>
            </plentry>
            </parml> </section>
    </refbody>
</reference>
