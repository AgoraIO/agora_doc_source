<reference id="getAssetAbsolutePath">
    <title><ph keyref="getAssetAbsolutePath" /></title>
    <shortdesc id="short"><ph id="shortdesc">Gets the actual absolute path of the asset through the relative path of the asset.</ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="getAssetAbsolutePath" />
            </keywords>
        </metadata>
    </prolog>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="android" outputclass="language-java" />
                <codeblock props="ios mac" outputclass="language-objectivec" />
                <codeblock props="windows" outputclass="language-cpp" />
                <codeblock props="electron" outputclass="language-typescript" />
                <codeblock props="unity" outputclass="language-csharp" />
                <codeblock props="rn" outputclass="language-typescript" />
                <codeblock props="flutter" outputclass="language-dart">Future&lt;String?&gt; getAssetAbsolutePath(String assetPath)</codeblock>
            </p>
        </section>
        <section id="parameters"><title>Parameters</title>
            <parml>
                <plentry>
                    <pt>assetPath</pt>
                    <pd>The resource path configured in the <ph>flutter</ph> -&gt; <ph>assets</ph> field of pubspec.yaml, for example: assets/Sound_Horizon.mp3.</pd>
                </plentry>
            </parml>
        </section>
        <section id="return_values">
            <title>Returns</title>
            <p>The actual path of the asset.</p>
        </section>
    </refbody>
</reference>