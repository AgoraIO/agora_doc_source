<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="api_onreceiveaudiopacket">
    <title><ph keyref="onReceiveAudioPacket"/></title>
    <shortdesc id="short"><ph id="shortdesc">Occurs when the local user receives an audio packet.</ph></shortdesc>
    <prolog>
        <metadata>
   <keywords>
       <indexterm keyref="onReceiveAudioPacket" />
   </keywords>
        </metadata>
    </prolog>
    <refbody><section id="prototype">
        
        <p outputclass="codeblock" conref="../conref/conref_rtc_api.dita#apidef/onReceiveAudioPacket"/>
        </section>
        <section id="parameters"><title>Parameters</title>
   <parml>
       <plentry>
  <pt>packet</pt>
  <pd>The received audio packet, see <xref keyref="Packet"/>.</pd>
       </plentry>
   </parml>
        </section>
        <section id="return_values">
   <title>Returns</title>
   <ul>
       <li><ph keyref="true"/>: The audio packet is received successfully.</li>
       <li><ph keyref="false"/>: The audio packet is discarded.</li>
   </ul>
        </section></refbody>
</reference>
