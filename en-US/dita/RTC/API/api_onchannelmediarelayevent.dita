didReceiveChannelMediaRelayEvent:(AgoraChannelMediaRelayEvent)event;</codeblock>
                <codeblock props="windows" outputclass="language-cpp">virtual void onChannelMediaRelayEvent(CHANNEL_MEDIA_RELAY_EVENT code) {
    }</codeblock>
