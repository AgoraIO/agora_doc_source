<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="api_getapplicationvolume">
    <title><ph keyref="getApplicationVolume"/></title>
    <shortdesc id="short"><ph id="shortdesc">Retrieves the volume of the application.</ph></shortdesc>
    <prolog>
        <metadata>
   <keywords>
       <indexterm keyref="getApplicationVolume" />
   </keywords>
        </metadata>
    </prolog>
    <refbody><section id="prototype">
        
        <p outputclass="codeblock" conref="../conref/conref_rtc_api.dita#apidef/getApplicationVolume"/>
        </section>
        <section id="parameters"><title>Parameters</title>
   <parml>
       <plentry>
  <pt>volume</pt>
  <pd>The application volume. The value ranges between 0 (lowest volume) and 255 (highest volume).</pd>
       </plentry>
   </parml>
        </section>
        <section id="return_values">
   <title>Returns</title>
   <ul>
       <li>0: Success.</li>
       <li>&lt; 0: Failure.</li>
   </ul>
        </section></refbody>
</reference>
