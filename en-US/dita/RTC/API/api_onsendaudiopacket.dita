<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="api_onsendaudiopacket">
    <title><ph keyref="onSendAudioPacket"/></title>
    <shortdesc id="short"><ph id="shortdesc">Occurs when the local user sends an audio packet.</ph></shortdesc>
    <prolog>
        <metadata>
   <keywords>
       <indexterm keyref="onSendAudioPacket" />
   </keywords>
        </metadata>
    </prolog>
    <refbody><section id="prototype">
        
        <p conref="../conref/conref_rtc_api.dita#apidef/onSendAudioPacket"/>
        </section>
        <section id="parameters"><title>Parameter</title>
   <parml>
       <plentry>
  <pt>packet</pt>
  <pd>The sent audio packet, see <xref keyref="Packet"/>.</pd>
       </plentry>
   </parml>
        </section>
        <section id="return_values">
   <title>Returns</title>
   <ul>
       <li><ph keyref="true"/>: The audio packet is sent successfully.</li>
       <li><ph keyref="false"/>: The audio packet is discarded.</li>
   </ul>
        </section></refbody>
</reference>
