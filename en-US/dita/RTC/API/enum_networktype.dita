<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="enum_networktype">
    <title><ph keyref="NETWORK_TYPE"/></title>
    <shortdesc id="short"><ph id="shortdesc">Network type.</ph></shortdesc>
    <refbody>
        <section id="parameters">
   <title>Enumerator</title>
   <parml>
       <plentry>
  <pt><ph keyref="NETWORK_TYPE_UNKNOWN"/></pt>
  <pd>-1: The network type is unknown.</pd>
       </plentry>
       <plentry>
  <pt><ph keyref="NETWORK_TYPE_DISCONNECTED"/></pt>
  <pd>0: The SDK disconnects from the network.</pd>
       </plentry>
       <plentry>
  <pt><ph keyref="NETWORK_TYPE_LAN"/></pt>
  <pd>1: The network type is LAN.</pd>
       </plentry>
       <plentry>
  <pt><ph keyref="NETWORK_TYPE_WIFI"/></pt>
  <pd>2: The network type is Wi-Fi (including hotspots).</pd>
       </plentry>
       <plentry>
  <pt><ph keyref="NETWORK_TYPE_MOBILE_2G"/></pt>
  <pd>3: The network type is mobile 2G.</pd>
       </plentry>
       <plentry>
  <pt><ph keyref="NETWORK_TYPE_MOBILE_3G"/></pt>
  <pd>4: The network type is mobile 3G.</pd>
       </plentry>
       <plentry>
  <pt><ph keyref="NETWORK_TYPE_MOBILE_4G"/></pt>
  <pd>5: The network type is mobile 4G.</pd>
       </plentry>
   </parml>
        </section></refbody>
</reference>
