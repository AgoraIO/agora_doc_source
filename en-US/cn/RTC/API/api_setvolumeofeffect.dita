<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="api_setvolumeofeffect">
    <title><ph keyref="setVolumeOfEffect"/></title>
    <shortdesc id="short"><ph id="shortdesc">Sets the volume of a specified audio effect.</ph></shortdesc>
    <prolog>
        <metadata>
   <keywords>
       <indexterm keyref="setVolumeOfEffect" />
   </keywords>
        </metadata>
    </prolog>
    <refbody><section id="prototype">
        
        <p conref="../conref/conref_rtc_api.dita#apidef/setVolumeOfEffect"/>
        </section>
        <section id="detailed_desc">
   
   <note outputclass="note">Call this method after <xref keyref="playEffect3"/>.</note>
        </section>
        <section id="parameters"><title>Parameter</title>
   <parml>
       <plentry>
  <pt>soundId</pt>
  <pd>Specifies the ID of the sound effect. Each audio effect has a unique ID.</pd>
       </plentry>
       <plentry>
  <pt>volume</pt>
           <pd>The playback volume. The value ranges from 0 to 100. The default value is 100, which represents the original volume.</pd>
       </plentry>
   </parml>
        </section>
        <section id="return_values">
   <title>Returns</title>
   <ul>
       <li>0: Success.</li>
       <li>&lt; 0: Failure.</li>
   </ul>
        </section></refbody>
</reference>
