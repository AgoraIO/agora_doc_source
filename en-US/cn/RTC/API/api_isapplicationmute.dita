<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="api_isapplicationmute">
    <title><ph keyref="isApplicationMute"/></title>
    <shortdesc id="short"><ph id="shortdesc">Gets the mute state of the application.</ph></shortdesc>
    <prolog>
        <metadata>
   <keywords>
       <indexterm keyref="isApplicationMute" />
   </keywords>
        </metadata>
    </prolog>
    <refbody><section id="prototype">
        
        <p conref="../conref/conref_rtc_api.dita#apidef/isApplicationMute"/>
        </section>
        <section id="parameters"><title>Parameter</title>
   <parml>
       <plentry>
  <pt>mute</pt>
  <pd><p>Gets the mute state of the application.<ul>
      <li><ph keyref="true"/>true: The application is muted.</li>
      <li><ph keyref="false"/>false: The application is not muted.</li>
  </ul></p></pd>
       </plentry>
   </parml>
        </section>
        <section id="return_values">
   <title>Returns</title>
   <ul>
       <li>0: Success.</li>
       <li>&lt; 0: Failure.</li>
   </ul>
        </section></refbody>
</reference>
