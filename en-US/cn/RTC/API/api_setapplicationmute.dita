<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="api_setapplicationmute">
    <title><ph keyref="setApplicationMute"/></title>
    <shortdesc id="short"><ph id="shortdesc">Mutes the application.</ph></shortdesc>
    <prolog>
        <metadata>
   <keywords>
       <indexterm keyref="setApplicationMute" />
   </keywords>
        </metadata>
    </prolog>
    <refbody><section id="prototype">
        
        <p conref="../conref/conref_rtc_api.dita#apidef/setApplicationMute"/>
        </section>
        <section id="parameters"><title>Parameter</title>
   <parml>
       <plentry>
  <pt>mute</pt>
  <pd>Whether to mute the app.<ul>
      <li><ph keyref="true"/>: Mute the app.</li>
      <li><ph keyref="false"/>: Do not mute the app.</li>
  </ul></pd>
       </plentry>
   </parml>
        </section>
        <section id="return_values">
   <title>Returns</title>
   <ul>
       <li>0: Success.</li>
       <li>&lt; 0: Failure.</li>
   </ul>
        </section></refbody>
</reference>
