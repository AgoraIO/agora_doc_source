   <parml>
       <plentry conkeyref="IChannel_onJoinChannelSuccess/rtcChannel">
  <pt></pt>
  <pd></pd>
       </plentry>
       <plentry conkeyref="onTokenPrivilegeWillExpire/token">
  <pt></pt>
  <pd></pd>
       </plentry>
   </parml>
        </section>
