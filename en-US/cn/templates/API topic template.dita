<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="${ask('Set API ID', String, 'ID', @apiID)}">
    <title><ph keyref="${answer(@apiID)}"/></title>
    <shortdesc id="short"><ph id="shortdesc"><?oxy-placeholder content="Provide a short description for this API."?></ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="${answer(@apiID)}" />
            </keywords>
        </metadata>
    </prolog>
    <refbody><section id="prototype">
        <p conref="../conref/conref_rtc_api.dita#apidef/${answer(@apiID)}"/>
        </section>
        <section id="detailed_desc">
            <p><?oxy-placeholder content="Details of this API, including any call limit, considerations, version information, deprecated information, and sample code."?></p>
        </section>
        <section id="parameters"><title>Parameter</title>
            <parml>
                <plentry>
                    <pt><?oxy-placeholder content="The name of the parameter."?></pt>
                    <pd><?oxy-placeholder content="The description of the parameter. Please specify if the parameter is optional, output, and has a default value or value range."?></pd>
                </plentry>
            </parml>
        </section>
        <section id="return_values">
            <title>Returns</title>
        </section></refbody>
</reference>
