<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="class_videostreamconfig">
    <title><ph keyref="AgoraEduContextVideoStreamConfig"/></title>
    <shortdesc id="short"><ph id="shortdesc">视频流配置。</ph></shortdesc>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="android" outputclass="language-kotlin">data class AgoraEduContextLocalStreamConfig(
        val width: Int,
        val height: Int,
        val frameRate: Int,
        val bitRate: Int,
        val mirror: Boolean
)</codeblock>
                <codeblock props="ios mac" outputclass="language-swift"/>
                <codeblock props="windows" outputclass="language-cpp"/>
                <codeblock props="web" outputclass="language-typescript"/>
            </p>
        </section>
        <section id="detailed_desc">
            <p/>
        </section>
        <section id="parameters">
            <title><text conref="../conref/conref_api_metadata.dita#metadata/property"/></title>
            <parml>
                <plentry>
                    <pt props="ios">dimensionWidth</pt>
                    <pt props="android">width</pt>
                    <pd>视频宽，<ph keyref="int"/> 型，单位为 pixel，默认值为 320。</pd>
                </plentry>
                <plentry>
                    <pt props="ios">dimensionHeight</pt>
                    <pt props="android">height</pt>
                    <pd>视频高，<ph keyref="int"/> 型，单位为 pixel，默认值为 240。</pd>
                </plentry>
                <plentry>
                    <pt>frameRate</pt>
                    <pd>视频帧率，<ph keyref="int"/> 型，单位为 fps，默认值为 15。</pd>
                </plentry>
                <plentry>
                    <pt>bitRate</pt>
                    <pd>视频码率，<ph keyref="int"/> 型，单位为 Kbps，默认值为 200。</pd>
                </plentry>
                <plentry>
                    <pt props="ios">isMirror</pt>
                    <pt props="android">mirror</pt>
                    <pd>视频是否镜像，<ph keyref="bool"/> 型，默认值为 <ph keyref="false"/>。</pd>
                </plentry>
            </parml>
        </section>
    </refbody>
</reference>
