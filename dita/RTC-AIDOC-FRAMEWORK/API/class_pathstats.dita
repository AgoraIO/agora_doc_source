                <codeblock props="unity" outputclass="language-csharp">    public class PathStats
    {
        public MultipathType type;

        public int txKBitRate;

        public int rxKBitRate;

        public PathStats()
        {
            this.type = MultipathType.Unknown;
            this.txKBitRate = 0;
            this.rxKBitRate = 0;
        }

        public PathStats(MultipathType t, int tx, int rx)
        {
            this.type = t;
            this.txKBitRate = tx;
            this.rxKBitRate = rx;
        }

