<reference id="enum_streampublishstate">
    <title><ph keyref="STREAM_PUBLISH_STATE"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="electron">发布状态。</ph><ph id="shortdesc" props="rn">发布状态。</ph><ph id="shortdesc" props="flutter">发布状态。</ph><ph id="shortdesc" props="unity">发布状态。</ph></shortdesc>
