    <shortdesc id="short"><ph id="shortdesc" props="electron">注册插件。</ph><ph id="shortdesc" props="rn">注册插件。</ph><ph id="shortdesc" props="flutter">注册插件。</ph><ph id="shortdesc" props="unity">注册插件。</ph></shortdesc>
    <prolog>
        <metadata>
