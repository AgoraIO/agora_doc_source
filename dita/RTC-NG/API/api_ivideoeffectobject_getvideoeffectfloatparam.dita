            <parml>
            <plentry props="native">
                <pt props="android apple cpp">option</pt>
                <pd props="android">参数类别的标识符。</pd>
                <pd props="apple">参数选项的类别。</pd>
                <pd props="cpp">参数所属的选项类别。</pd>
            </plentry>
            <plentry props="cpp native">
                <pt props="native">key</pt>
                <pd props="native">参数的键名。</pd>
            </plentry>
            <plentry props="cpp">
