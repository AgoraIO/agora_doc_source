    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="currentUrl"/>
            </keywords>
        </metadata>
    </prolog>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
            <codeblock props="android" outputclass="language-java">public String currentUrl()</codeblock>
            <codeblock props="hmos" outputclass="language-arkts"/>
