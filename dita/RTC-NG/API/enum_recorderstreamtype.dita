<reference id="enum_recorderstreamtype">
    <title><ph keyref="RecorderStreamType"/></title>
    <shortdesc id="short"><ph id="shortdesc">The type of video stream to be recorded.</ph></shortdesc>
    <refbody>
        <section id="parameters">
            <title>Enumeration value</title>
            <parml>
                <plentry>
                    <pt><ph keyref="RTC"/></pt>
                    <pd>0: (Default) Video stream in the channel.</pd>
                </plentry>
                <plentry>
                    <pt><ph keyref="PREVIEW"/></pt>
                    <pd>1: Preview the video stream locally before joining the channel.</pd>
                </plentry>
            </parml>
        </section>
    </refbody>
</reference>
