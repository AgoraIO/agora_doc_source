<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="enum_screencapturesourcetype">
    <title><ph keyref="ScreenCaptureSourceType"/></title>
    <shortdesc id="short"><ph id="shortdesc">共享目标的类型。在 <xref keyref="ScreenCaptureSourceInfo"/>
            中设置。</ph></shortdesc>
    <refbody>
        <section id="detailed_desc">
            <dl outputclass="since" props="rtc">
                <dlentry props="native">
                    <dt>自从</dt>
                    <dd>v3.5.2</dd>
                </dlentry>
            </dl>
        </section>
        <section id="parameters">
            <title>枚举值</title>
            <parml>
                <plentry>
                    <pt><ph keyref="ScreenCaptureSourceType_Unknown"/></pt>
                    <pd>-1：未知。</pd>
                </plentry>
                <plentry>
                    <pt><ph keyref="ScreenCaptureSourceType_Window"/></pt>
                    <pd>0：共享目标为某一个窗口。</pd>
                </plentry>
                <plentry>
                    <pt><ph keyref="ScreenCaptureSourceType_Screen"/></pt>
                    <pd>1：共享目标为某一个显示器的屏幕。</pd>
                </plentry>
                <plentry>
                    <pt><ph keyref="ScreenCaptureSourceType_Custom"/></pt>
                    <pd>2：预留参数。</pd>
                </plentry>
            </parml>
        </section>
    </refbody>
</reference>
