        <section id="prototype">
            <p outputclass="codeblock">
            <codeblock props="android" outputclass="language-java">public Constants.ErrorCode code()</codeblock>
            <codeblock props="hmos" outputclass="language-arkts"/>
            <codeblock props="ios mac" outputclass="language-objectivec">- (AgoraRteErrorCode)code;</codeblock>
            <codeblock props="cpp unreal" outputclass="language-cpp">ErrorCode Code() const</codeblock>
            <codeblock props="bp" outputclass="language-cpp"/>
