            <plentry conkeyref="onJoinChannelSuccess/engine" props="ios mac">
                <pt />
                <pd />
            </plentry>
            <plentry props="unity electron rn" conkeyref="joinChannelEx/connection">
                <pt />
                <pd />
            </plentry>
            <plentry id="token">
                <pt>token</pt>
                <pd>即将服务失效的 Token。</pd>
            </plentry>
            </parml> </section>
    </refbody>
