    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="getState"/>
            </keywords>
        </metadata>
    </prolog>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock" props="rtc-ng">
                <codeblock props="unity" outputclass="language-csharp">public abstract MEDIA_PLAYER_STATE GetState(int playerId);</codeblock>
            </p>
            <p outputclass="codeblock">
                <codeblock props="android" outputclass="language-java">Constants.MediaPlayerState getState();
</codeblock>
                <codeblock props="ios mac" outputclass="language-objectivec">- (AgoraMediaPlayerState)getPlayerState;
</codeblock>
