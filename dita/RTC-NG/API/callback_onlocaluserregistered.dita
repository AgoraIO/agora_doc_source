                <pd/>
            </plentry>
            <plentry>
                <pt>uid</pt>
                <pd>本地用户的 ID。</pd>
            </plentry>
            <plentry>
                <pt>userAccount</pt>
                <pd>本地用户的 User Account。</pd>
            </plentry>
            </parml> </section>
