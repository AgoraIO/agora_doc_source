<reference id="api_irtcengine_getwindowsinfo">
    <title><ph keyref="getWindowsInfo"/></title>
    <shortdesc id="short"><ph id="shortdesc">获取窗口信息。</ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="getWindowsInfo"/>
            </keywords>
        </metadata>
    </prolog>
    <refbody>
