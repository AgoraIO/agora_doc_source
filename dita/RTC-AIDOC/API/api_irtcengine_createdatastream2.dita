    <shortdesc id="short"><ph id="shortdesc" props="cpp">创建一个数据流。</ph><ph id="shortdesc" props="android">创建一个数据流。</ph><ph id="shortdesc" props="ios">创建一个数据流。</ph><ph id="shortdesc" props="mac">创建一个数据流。</ph></shortdesc>
    <prolog>
        <metadata>
