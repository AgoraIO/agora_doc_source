<reference id="api_imediaplayer_unloadsrc">
    <title><ph keyref="unloadSrc"/></title>
    <shortdesc id="short"><ph id="shortdesc">卸载预加载的媒体资源。</ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="unloadSrc"/>
            </keywords>
        </metadata>
