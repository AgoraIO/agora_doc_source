    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="cpp" outputclass="language-cpp">virtual int unloadSrc(const char* src) = 0;</codeblock>
                <codeblock props="android" outputclass="language-java">int unloadSrc(String src);</codeblock>
                <codeblock props="ios" outputclass="language-objectivec">- (int)unloadSrc:(NSString *)src NS_SWIFT_NAME(unloadSrc(_:));</codeblock>
                <codeblock props="mac" outputclass="language-objectivec">- (int)unloadSrc:(NSString *)src NS_SWIFT_NAME(unloadSrc(_:));</codeblock>
