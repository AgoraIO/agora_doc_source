<reference id="enum_mediaplayerstate">
    <title><ph keyref="MEDIA_PLAYER_STATE"/></title>
    <shortdesc id="short"><ph id="shortdesc" props="cpp">播放状态。</ph><ph id="shortdesc" props="android">播放状态。</ph><ph id="shortdesc" props="ios">播放状态。</ph><ph id="shortdesc" props="mac">播放状态。</ph></shortdesc>
