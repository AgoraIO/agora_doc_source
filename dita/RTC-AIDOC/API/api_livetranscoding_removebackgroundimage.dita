<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="api_livetranscoding_removebackgroundimage">
    <title><ph keyref="removeBackgroundImage"/></title>
    <shortdesc id="short"><ph id="shortdesc">从背景图像列表中移除指定的背景图像。</ph></shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="removeBackgroundImage"/>
            </keywords>
        </metadata>
    </prolog>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="android" outputclass="language-java">public boolean removeBackgroundImage(AgoraImage backgroundImage)</codeblock>
            </p>
        </section>
        <section id="detailed_desc" deliveryTarget="details" otherprops="no-title"/>
        <section id="parameters" deliveryTarget="details" props="android">
            <title>参数</title>
            <parml>
                <plentry props="android">
                    <pt>backgroundImage</pt>
                    <pd>要移除的背景图像。详见 <xref keyref="RtcImage"/>。</pd>
                </plentry>
            </parml>
        </section>
        <section id="return_values" props="android">
            <title>返回值</title>
            <p props="android">
                <ul>
                    <li><xref keyref="true"/>：移除成功。</li>
                    <li><xref keyref="false"/>：移除失败。</li>
                </ul>
            </p>
        </section>
    </refbody>
</reference>