                <codeblock props="android" outputclass="language-java">Constants.MediaPlayerState getState();</codeblock>
                <codeblock props="ios" outputclass="language-objectivec">- (AgoraMediaPlayerState)getPlayerState NS_SWIFT_NAME(getPlayerState());</codeblock>
                <codeblock props="mac" outputclass="language-objectivec">- (AgoraMediaPlayerState)getPlayerState NS_SWIFT_NAME(getPlayerState());</codeblock>
