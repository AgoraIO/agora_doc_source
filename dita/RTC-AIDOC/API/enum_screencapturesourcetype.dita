<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="enum_screencapturesourcetype">
    <title>
        <ph keyref="ScreenCaptureSourceType"/>
  </title>
    <shortdesc id="short">
        <ph id="shortdesc">共享目标的类型。</ph>
  </shortdesc>
    <refbody>
        <section id="detailed_desc" deliveryTarget="details" otherprops="no-title">
            <p props="cpp">在 <codeph>ScreenCaptureSourceInfo</codeph> 中设置。</p>
        </section>
        <section id="parameters" deliveryTarget="details">
            <title>枚举值</title>
            <parml>
                <plentry props="cpp">
                    <pt>ScreenCaptureSourceType_Unknown</pt>
                    <pd>（-1）：未知类型。</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>ScreenCaptureSourceType_Window</pt>
                    <pd>（0）：共享目标为窗口。</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>ScreenCaptureSourceType_Screen</pt>
                    <pd>（1）：共享目标为某个显示器的屏幕。</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>ScreenCaptureSourceType_Custom</pt>
                    <pd>（2）：保留参数。</pd>
                </plentry>
            </parml>
        </section>
    </refbody>
</reference>