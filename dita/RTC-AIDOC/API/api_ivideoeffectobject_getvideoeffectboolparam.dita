            <title>参数</title>
            <parml>
                <plentry props="cpp">
                    <pt>option</pt>
                    <pd>参数所属的选项类别。</pd>
                </plentry>
                <plentry props="android cpp">
                    <pt>key</pt>
                    <pd>参数的键名。</pd>
                </plentry>
