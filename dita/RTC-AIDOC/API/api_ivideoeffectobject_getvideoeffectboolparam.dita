            <p outputclass="codeblock">
                <codeblock props="cpp" outputclass="language-cpp">virtual bool getVideoEffectBoolParam(const char* option, const char* key) = 0;</codeblock>
                <codeblock props="android" outputclass="language-java">boolean getVideoEffectBoolParam(String option, String key);</codeblock>
                <codeblock props="ios" outputclass="language-objectivec">- (BOOL)getVideoEffectBoolParamWithOption:(NSString *)option
                                    key:(NSString *)key
NS_SWIFT_NAME(getVideoEffectBoolParam(option:key:));</codeblock>
                <codeblock props="mac" outputclass="language-objectivec">- (BOOL)getVideoEffectBoolParamWithOption:(NSString *)option
