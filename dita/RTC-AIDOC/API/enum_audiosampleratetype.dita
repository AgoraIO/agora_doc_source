            <parml>
                <plentry props="cpp">
                    <pt>AUDIO_SAMPLE_RATE_32000</pt>
                    <pd>（32000）：32 kHz。</pd>
                </plentry>
                <plentry props="cpp">
                    <pt>AUDIO_SAMPLE_RATE_44100</pt>
                    <pd>（44100）：44.1 kHz。</pd>
