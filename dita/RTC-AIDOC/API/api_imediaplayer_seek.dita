            <parml>
                <plentry props="cpp">
                    <pt>newPos</pt>
                    <pd>新的播放位置，单位为毫秒。</pd>
                </plentry>
                <plentry props="android">
                    <pt>newPos</pt>
                    <pd>新的播放位置，单位为毫秒。</pd>
                </plentry>
                <plentry props="ios">
                    <pt>position</pt>
                    <pd>新的播放位置（单位为毫秒）。</pd>
                </plentry>
                <plentry props="mac">
                    <pt>position</pt>
                    <pd>新的播放位置（单位为毫秒）。</pd>
                </plentry>
            </parml>
        </section>
