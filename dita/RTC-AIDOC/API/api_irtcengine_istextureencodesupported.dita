<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE reference PUBLIC "-//OASIS//DTD DITA Reference//EN" "reference.dtd">
<reference id="api_irtcengine_istextureencodesupported">
    <title>
    <ph keyref="isTextureEncodeSupported"/>
  </title>
    <shortdesc id="short">
    <ph id="shortdesc">检查视频是否支持 <codeph>Texture</codeph> 编码。</ph>
  </shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="isTextureEncodeSupported"/>
            </keywords>
        </metadata>
    </prolog>
    <refbody>
        <section id="prototype">
            <p outputclass="codeblock">
                <codeblock props="android" outputclass="language-java">public abstract boolean isTextureEncodeSupported();</codeblock>
            </p>
        </section>
        <section id="timing" deliveryTarget="details">
            <title>调用时机</title>
            <p props="android">加入频道前后均可调用。</p>
        </section>
        <section id="restriction" deliveryTarget="details">
            <title>调用限制</title>
            <p props="android">无。</p>
        </section>
        <section id="return_values">
            <title>返回值</title>
            <p props="android">
                <ul>
                    <li><xref keyref="true"/>：支持 <codeph>Texture</codeph> 编码。</li>
                    <li><xref keyref="false"/>：不支持 <codeph>Texture</codeph> 编码。</li>
                </ul>
            </p>
        </section>
    </refbody>
</reference>