    <title>
    <ph keyref="resumeAudioMixing"/>
  </title>
    <shortdesc id="short">
    <ph id="shortdesc">恢复播放并混音音乐文件。</ph>
  </shortdesc>
    <prolog>
        <metadata>
            <keywords>
                <indexterm keyref="resumeAudioMixing"/>
            </keywords>
