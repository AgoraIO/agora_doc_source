  <div>
    <a:documentation>DOMAIN EXTENSION PATTERNS</a:documentation>

    <define name="hi-d-ph">
      <choice>
        <ref name="b.element"/>
        <ref name="i.element"/>
        <ref name="line-through.element"/>
        <ref name="overline.element"/>
        <ref name="sup.element"/>
        <ref name="sub.element"/>
        <ref name="tt.element"/>
        <ref name="u.element"/>
      </choice>
    </define>

    <define name="ph" combine="choice">
      <ref name="hi-d-ph"/>
    </define>
  </div>
