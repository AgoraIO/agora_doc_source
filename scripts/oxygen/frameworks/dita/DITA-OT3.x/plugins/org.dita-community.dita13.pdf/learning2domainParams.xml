<?xml version="1.0" encoding="UTF-8"?>
<extension xmlns:if="ant:if">
  <param name="lc-number-questions" expression="${arg.lc.html.numberQuestions}" if:set="arg.lc.html.numberQuestions" xmlns:if="ant:if"/>  
  <param name="lc-show-feedback" expression="${arg.lc.html.showFeedback}" if:set="arg.lc.html.showFeedback" xmlns:if="ant:if"/>  
