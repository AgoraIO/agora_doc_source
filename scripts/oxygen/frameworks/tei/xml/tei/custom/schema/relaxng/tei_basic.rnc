     attribute target { xsd:anyURI }?)
    >> sch:pattern [
         id =
           "tei_basic-rt-target-rt-target-not-span-constraint-report-7"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:rt/@target"
           "\x{a}" ~
