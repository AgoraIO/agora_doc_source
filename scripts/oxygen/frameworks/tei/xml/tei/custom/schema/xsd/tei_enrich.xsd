    </xs:annotation>
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="tei:glyphName"/>
        <xs:element ref="tei:charProp"/>
        <xs:element ref="tei:unicodeProp"/>
        <xs:element ref="tei:unihanProp"/>
        <xs:element ref="tei:localProp"/>
